<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.enclica.furryfan_mobile">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FurryfanMobile">
        <activity
            android:name=".pages.Upload_web_window"
            android:exported="false"
            android:label="@string/title_activity_upload_web_window"
            android:theme="@style/Theme.FurryfanMobile" />
        <activity
            android:name=".pages.Profile_page"
            android:exported="false"
            android:label="@string/title_activity_profile_page"
            android:theme="@style/Theme.FurryfanMobile.NoActionBar" />
        <activity android:name=".pages.Settings" />
        <activity android:name=".pages.imageviewer" />
        <activity android:name=".pages.Videoplayer" />
        <activity
            android:name=".Newuserpage"
            android:label="@string/title_activity_newuserpage"
            android:theme="@style/Theme.FurryfanMobile.NoActionBar" />
        <activity
            android:name=".usershome"
            android:label="@string/title_activity_usershome"
            android:theme="@style/Theme.FurryfanMobile.NoActionBar" />
        <activity android:name=".signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>